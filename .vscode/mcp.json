{
  "servers": {
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"],
      "type": "stdio"
    },
    "prisma": {
      "url": "https://mcp.prisma.io/mcp",
      "type": "http"
    },
    "gcloud": {
      "command": "npx",
      "args": ["-y", "@google-cloud/gcloud-mcp"],
      "type": "stdio"
    },
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"],
      "type": "stdio"
    },
    "Vercel": {
      "url": "https://mcp.vercel.com",
      "type": "http"
    }
  },
  "inputs": []
}
